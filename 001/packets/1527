var nCntSelectNotConfirm = 0;
function OnTreeClick(evt) {
var src = window.event != window.undefined ? window.event.srcElement : evt.target;
var isChkBoxClick = (src.tagName.toLowerCase() == "input" && src.type == "checkbox");
if (isChkBoxClick) {
if (src.checked == true) {
nCntSelectNotConfirm += 1;
}
else {
nCntSelectNotConfirm -= 1;
}
document.getElementById("lblCountSelectNotConfirm").innerHTML = nCntSelectNotConfirm.toString() + " آیتم ";
}
if (src.tagName.toLowerCase() == "span") {
var id = src.parentElement.id;
// با کلیک روی کپشنی که با چک باکس شروع شده است، چک باکس هم انتخاب شود 
var cGetId = id.replace('TreeMenut', 'TreeMenun') + "CheckBox";
var ctrl = document.getElementById(cGetId);
if (ctrl != null) {
if (ctrl.checked == true) {
ctrl.checked = false;
nCntSelectNotConfirm -= 1;
}
else {
ctrl.checked = true;
nCntSelectNotConfirm += 1;
}
document.getElementById("lblCountSelectNotConfirm").innerHTML = nCntSelectNotConfirm.toString() + " آیتم ";
}
return false;
}
if (src.tagName.toLowerCase() == "a") {
return false;
}
}
