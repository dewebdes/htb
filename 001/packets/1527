function ValidateEmailId() {
var value = document.getElementById("ctl00_PageContent_txtEmailAddress").value;
if (value != '') {
var atposition = value.indexOf("@");
var dotposition = value.lastIndexOf(".");
if (atposition < 1 || dotposition < atposition + 2 || dotposition + 2 >= value.length) {
//$.alert("نشانی پست الکترونیک  نامعتبر است");
iziToast.error({ title: 'هشدار', message: 'نشانی پست الکترونیک  نامعتبر است', iconText: 'error_outline' });
document.getElementById("ctl00_PageContent_txtEmailAddress").focus();
return false;
}
}
else {
return true;
}
return true;
}
