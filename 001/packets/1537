            function trgger(e) {
              document.getElementById(e).click();
              new Showprogress();
            }
            function Showprogress() {
              var indicator = document.querySelector("div.indicator");
              var preview = document.querySelector("div.preview");
              var divand = document.createElement("div");
              divand.setAttribute("id", "progressand");
              divand.style.display = "none";
              indicator.appendChild(divand);
              var div = document.createElement("div");
              div.setAttribute("class", "progress");
              divand.appendChild(div);
              var dynamic = document.createElement("div");
              dynamic.setAttribute("id", "dynamic");
              dynamic.setAttribute(
                "class",
                "progress-bar progress-bar-striped bg-info progress-bar-animated"
              );
              dynamic.setAttribute("role", "progressbar");
              dynamic.setAttribute("aria-valuenow", "0");
              dynamic.setAttribute("aria-valuemin", "0");
              dynamic.setAttribute("aria-valuemax", "100");
              dynamic.style.width = "0%";
              div.appendChild(dynamic);
              var percent = document.createElement("span");
              percent.setAttribute("class", "percent");
              percent.textContent = "0%";
              dynamic.appendChild(percent);
              var load = document.createElement("div");
              load.setAttribute("class", "ml-auto mr-auto lds-contuner");
              load.setAttribute("id", "loader");
              load.style.display = "none";
              preview.appendChild(load);
              var loader = document.createElement("div");
              loader.setAttribute("class", "lds-ellipsis");
              load.appendChild(loader);
              for (let x = 0; x < 4; x++) {
                var divm = document.createElement("div");
                loader.appendChild(divm);
              }
            }
            var inputHiddenFiled;
            function previewFiles() {
              var preview = document.querySelector("div.preview");
              var files = document.querySelector("input[type=file]").files;
              function readAndPreview(file) {
                if (/\.(pdf|rar|)$/i.test(file.name)) {
                  if (file.size > 1 * 1024 * 1024) {
                    // MAX 1 mb  for pdf and rar files
                    return iziToast.error({
                      title: "ناموفق",
                      message: "حداکثر یک مگابایت",
                      iconText: "error_outline",
                    });
                  }
                }
                // get accepted format file and check size and resize image format
                if (
                  /\.(jpe?g|png|gif|mp4|webm|ogg|ogv|mp3|wav|pdf|rar)$/i.test(
                    file.name
                  )
                ) {
                  if (file.size < 25 * 1024 * 1024) {
                    // MAX 25 mb
                    var reader = new FileReader();
                    var spc = "\u00A0";
                    function returnFileSize(n) {
                      if (n < 1024) {
                        return n + " octets";
                      } else if (n >= 1024 && n < 1048576) {
                        return (n / 1024).toFixed(1) + spc + "Ko";
                      } else if (n >= 1048576) {
                        return (n / 1048576).toFixed(1) + spc + "Mo";
                      }
                    }
                    function abortRead() {
                      reader.abort();
                    }
                    reader.onloadstart = function (e) {
                      document.querySelector("#progressand").style.display =
                        "block";
                      document.querySelector("#loader").style.display = "block";
                    };
                    reader.onprogress = function (e) {
                      if (e.lengthComputable) {
                        var percentLoaded = Math.round(
                          (e.loaded / e.total) * 99
                        );
                        if (percentLoaded < 99) {
                          document.querySelector("#dynamic").style.width =
                            percentLoaded + "%";
                          document.querySelector(".percent").textContent =
                            percentLoaded + "%";
                        }
                      }
                    };
                    reader.onloadend = function (e) {
                      document.querySelector("#dynamic").style.width = "100%";
                      document.querySelector(".percent").textContent = "100%";
                      setTimeout(function () {
                        $("#progressand").remove();
                        $("#loader").remove();
                      }, 900);
                    };
                    reader.onabort = function (e) {
                      return iziError(failed, "File read cancelled");
                    };
                    reader.addEventListener("error", function () {
                      return iziError(
                        failed,
                        "Error occurred reading file: ${file.name}"
                      );
                    });
                    reader.addEventListener(
                      "load",
                      function () {
                        //save Base64
                        strImgBase64 = this.result;
                        setValueInHidenField();
                        if (lMax10Files) {
                          return;
                        }
                        //alert('onload' + inputHiddenFiled.id);
                        var div = document.createElement("div");
                        div.setAttribute("class", "Imgpreview mb-3"); // animated zoomIn
                        preview.appendChild(div);
                        var i = document.createElement("i");
                        i.setAttribute("class", "material-icons remove");
                        i.textContent = "close";
                        i.setAttribute("hf", inputHiddenFiled.id);
                        div.appendChild(i);
                        if (/\.(jpe?g|png|gif|)$/i.test(file.name)) {
                          var image = new Image();
                          //image.addEventListener("load", function () {/*image.width+'×'+image.height*/})
                          image.setAttribute(
                            "data-tippy-content",
                            file.name +
                              spc +
                              "\u002f" +
                              spc +
                              "file size" +
                              spc +
                              returnFileSize(file.size)
                          );
                          image.setAttribute(
                            "onclick",
                            'this.classList.toggle("Imgpreview-zoom");'
                          );
                          image.src = this.result;
                          image.setAttribute("hf", inputHiddenFiled.id);
                          div.appendChild(image);
                          var canvas = document.getElementById("canvas");
                          var ctx = canvas.getContext("2d");
                          //width > 1000 && size>700kb
                          image.onload = function () {
                            var imageWidth = 0,
                              imageHeight = 0;
                            // تصویر چون داخل کلاسی هست که با استایل سایز می گیرد از خط های ذیل برای داشتن اندازه عکس استفاده شده است
                            // for get original image size
                            document.getElementById("myImg").src = image.src;
                            imageWidth = document.getElementById("myImg").width;
                            imageHeight =
                              document.getElementById("myImg").height;
                            // اگر عکس بیشتر از 700 کیلوبایت بود و وید آن از 1000 بیشتر بود با جاوااسکریپت انرا کوچک کن
                            var fileSize = parseFloat(file.size / 1024).toFixed(
                              2
                            );
                            if (imageWidth > 700 && fileSize > 700) {
                              //resize image
                              // set size proportional to image
                              canvas.height =
                                canvas.width * (imageHeight / imageWidth);
                              // step 1 - resize to 50%
                              var oc = document.createElement("canvas"),
                                octx = oc.getContext("2d");
                              oc.width = imageWidth * 0.5;
                              oc.height = imageHeight * 0.5;
                              octx.drawImage(image, 0, 0, oc.width, oc.height);
                              // step 2
                              octx.drawImage(
                                oc,
                                0,
                                0,
                                oc.width * 0.5,
                                oc.height * 0.5
                              );
                              // step 3, resize to final size
                              ctx.drawImage(
                                oc,
                                0,
                                0,
                                oc.width * 0.5,
                                oc.height * 0.5,
                                0,
                                0,
                                canvas.width,
                                canvas.height
                              );
                              //کردن نتیجه کار را در هیدن  فیلد مربوطه ذخیره کن resizeبعد از
                              //alert(inputHiddenFiled.id);
                              var id = this.getAttribute("hf");
                              document.getElementById(id).value =
                                canvas.toDataURL(); //get new Base64Image
                              //alert('resize' + id);
                              //inputHiddenFiled.value = canvas.toDataURL();//get new Base64Image
                            }
                          };
                        } else if (/\.(mp4|webm|ogg|ogv)$/i.test(file.name)) {
                          var video = document.createElement("video");
                          video.setAttribute("width", "100%");
                          video.setAttribute(
                            "data-tippy-content",
                            file.name +
                              spc +
                              "\u002f" +
                              spc +
                              "file size" +
                              spc +
                              returnFileSize(file.size)
                          );
                          video.setAttribute(
                            "onclick",
                            'this.classList.toggle("Imgpreview-zoom");'
                          );
                          video.muted = false;
                          video.volume = 0;
                          video.autoplay = true;
                          video.loop = true;
                          video.preload = "auto";
                          video.src = this.result;
                          div.appendChild(video);
                        } else if (/\.(mp3|wav|ogg)$/i.test(file.name)) {
                          var audio = document.createElement("audio");
                          audio.setAttribute(
                            "data-tippy-content",
                            file.name +
                              spc +
                              "\u002f" +
                              spc +
                              "file size" +
                              spc +
                              returnFileSize(file.size)
                          );
                          audio.setAttribute("controlslist", "nodownload");
                          audio.controls = true;
                          audio.preload = "auto";
                          audio.src = this.result;
                          div.appendChild(audio);
                        } else {
                          //for other files ex: pdf , rar file
                          var extension = file.name
                            .split(".")
                            .pop()
                            .toLowerCase();
                          var image = new Image();
                          image.setAttribute(
                            "data-tippy-content",
                            file.name +
                              spc +
                              "\u002f" +
                              spc +
                              "file size" +
                              spc +
                              returnFileSize(file.size)
                          );
                          image.setAttribute(
                            "onclick",
                            'this.classList.toggle("Imgpreview-zoom");'
                          );
                          if (extension == "pdf") {
                            image.src = "../../../Images/PDFFile.png";
                          } else if (extension == "rar") {
                            image.src = "../../../Images/RARFile.png";
                          }
                          div.appendChild(image);
                        }
                        tippy([image, video, audio]);
                        $(i).click(function () {
                          $(div).removeClass("zoomIn").addClass("flipOutX"); // hinge
                          // delete file
                          setTimeout(function () {
                            ClearValueInHidenField($(i)[0]);
                            $(i).parent(div).remove();
                          }, 500);
                        });
                      },
                      false
                    );
                    reader.readAsDataURL(file);
                  } else {
                    return iziToast.error({
                      title: "ناموفق",
                      message: "سایز فایل بزرگ هست",
                      iconText: "error_outline",
                    });
                  }
                } else {
                  return iziToast.error({
                    title: "ناموفق",
                    message:
                      "لطفا فایل معتبری تهیه کنید، قالبهای پذیرفته شده شامل   .png ، .jpg و .gif , pdf, rar  است ",
                    iconText: "error_outline",
                  });
                }
              }
              if (files) {
                [].forEach.call(files, readAndPreview);
              }
            }
            autosize(document.querySelectorAll(".autosize"));
            if (document.getElementsByClassName("uploadFile")[0] != undefined) {
              $(".scroll-bar").each(function () {
                var ps = new PerfectScrollbar($(this)[0], {
                  wheelSpeed: 0.4,
                  minScrollbarLength: 100,
                  suppressScrollX: true,
                });
              });
              // var psA = new PerfectScrollbar('.uploadFile');
            }
            iziToast.settings({
              timeout: 5000,
              drag: true,
              pauseOnHover: true,
              animateInside: true,
              theme: "light",
              icon: "material-icons",
              transitionIn: "bounceInLeft",
              progressBarColor: "#ff0000",
              transitionOut: "flipOutX",
              position: "topRight",
            });
            function ClearValueInHidenField(obj) {
              var id = obj.getAttribute("hf");
              document.getElementById(id).value = "";
            }
            var lMax10Files = false;
            function setValueInHidenField() {
              var execution = true;
              if (_hfBase64img1.value == "") {
                _hfBase64img1.value = strImgBase64;
                inputHiddenFiled = _hfBase64img1;
                execution = false;
              }
              if (execution == true && _hfBase64img2.value == "") {
                _hfBase64img2.value = strImgBase64;
                inputHiddenFiled = _hfBase64img2;
                execution = false;
              }
              if (execution == true && _hfBase64img3.value == "") {
                _hfBase64img3.value = strImgBase64;
                inputHiddenFiled = _hfBase64img3;
                execution = false;
              }
              if (execution == true && _hfBase64img4.value == "") {
                _hfBase64img4.value = strImgBase64;
                inputHiddenFiled = _hfBase64img4;
                execution = false;
              }
              if (execution == true && _hfBase64img5.value == "") {
                _hfBase64img5.value = strImgBase64;
                inputHiddenFiled = _hfBase64img5;
                execution = false;
              }
              if (execution == true && _hfBase64img6.value == "") {
                _hfBase64img6.value = strImgBase64;
                inputHiddenFiled = _hfBase64img6;
                execution = false;
              }
              if (execution == true && _hfBase64img7.value == "") {
                _hfBase64img7.value = strImgBase64;
                inputHiddenFiled = _hfBase64img7;
                execution = false;
              }
              if (execution == true && _hfBase64img8.value == "") {
                _hfBase64img8.value = strImgBase64;
                inputHiddenFiled = _hfBase64img8;
                execution = false;
              }
              if (execution == true && _hfBase64img9.value == "") {
                _hfBase64img9.value = strImgBase64;
                inputHiddenFiled = _hfBase64img9;
                execution = false;
              }
              if (execution == true && _hfBase64img10.value == "") {
                _hfBase64img10.value = strImgBase64;
                inputHiddenFiled = _hfBase64img10;
                execution = false;
              }
              lMax10Files = execution;
              if (execution) {
                return iziToast.info({
                  title: "توجه",
                  message: "حداکثر 10 فایل",
                  iconText: "warning",
                });
              }
            }
            function ShowPreviewFiles() {
              var preview = document.querySelector("div.preview");
              preview.innerHTML = _hfPreview.value;
              if (_hfCntSelectNotConfirm.value != "") {
                nCntSelectNotConfirm = parseInt(_hfCntSelectNotConfirm.value);
                document.getElementById("lblCountSelectNotConfirm").innerHTML =
                  nCntSelectNotConfirm.toString() + " آیتم ";
              }
            }
            if (document.getElementsByClassName("uploadFile")[0] != undefined) {
              setTimeout(ShowPreviewFiles(), 100);
            }
            function RemoveAllFiles() {
              var preview = document.querySelector("div.preview");
              preview.innerHTML = "";
              _hfPreview.value = "";
              _hfBase64img1.value = "";
              _hfBase64img2.value = "";
              _hfBase64img3.value = "";
              _hfBase64img4.value = "";
              _hfBase64img5.value = "";
              _hfBase64img6.value = "";
              _hfBase64img7.value = "";
              _hfBase64img8.value = "";
              _hfBase64img9.value = "";
              _hfBase64img10.value = "";
            }
            function OnSubmit() {
              document.getElementById("myImg").src = "";
              var preview = document.querySelector("div.preview");
              _hfPreview.value = preview.innerHTML;
            }
            var _root = "/";
            function DoSearch(swich) {
              switch (swich) {
                case 1:
                  document.drpSearch = _ddlBrand;
                  break;
                case 2:
                  document.drpSearch = _ddlApp;
                  break;
                case 3:
                  document.drpSearch = _ddlModel;
                  break;
                case 4:
                  document.drpSearch = _ddlStateName;
                  break;
                case 5:
                  document.drpSearch = _ddlCityName;
                  break;
              }
              createModalPopup(
                _root + "Controls/Loader/ControlSearch.aspx",
                "انتخاب پیش شرح ",
                250,
                210,
                1,
                1,
                1
              );
            }
