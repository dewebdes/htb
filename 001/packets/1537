            function changeGauranteeStatus(isGaurantee) {
              if (
                _hfDisableGauranteeStatus != null &&
                _hfDisableGauranteeStatus.value == "true"
              ) {
                _radioGaurantee.checked = _radioGaurantee.defaultChecked;
                _radioNotGaurantee.checked = _radioNotGaurantee.defaultChecked;
                return false; // امکان تغییر نداشته باشد.
              }
              var elm = document.getElementById(
                "ctl00_PageContent_ddlContractor"
              );
              if (elm) {
                if (isGaurantee) {
                  elm.className = "mandatory";
                  elm.disabled = false;
                } else {
                  elm.className = "readonly";
                  elm.disabled = true;
                }
              }
              elm = document.getElementById(
                "ctl00_PageContent_txtGauranteeCode"
              );
              if (elm) {
                if (isGaurantee) {
                  elm.className = "mandatory";
                  elm.readOnly = false;
                } else {
                  elm.className = "readonly";
                  elm.readOnly = true;
                }
              }
              elm = document.getElementById("ctl00_PageContent_txtShamsiDate");
              if (elm) {
                if (isGaurantee) {
                  elm.className = "optional";
                  elm.readOnly = false;
                } else {
                  elm.className = "readonly";
                  elm.readOnly = true;
                }
              }
            }
            changeGauranteeStatus(true);
