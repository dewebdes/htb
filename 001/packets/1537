          <script>
            function CheckForm() {
              var execution = false;
              execution = ValidateEmailId();
              if (execution) {
                execution = checkMobileNo(_txtMobileNo);
              }
              if (execution) {
                if (_txtVrDes.value == "") {
                  // $.alert("شرح درخواست وارد نشده است");
                  iziToast.error({
                    title: "ناموفق",
                    message: "شرح درخواست وارد نشده است",
                    iconText: "error_outline",
                  });
                  execution = false;
                  _txtVrDes.focus();
                }
              }
              OnSubmit();
              _hfCntSelectNotConfirm.value = nCntSelectNotConfirm.toString();
              return execution;
            }
            function ValidateEmailId() {
              var value = document.getElementById(
                "ctl00_PageContent_txtEmailAddress"
              ).value;
              if (value != "") {
                var atposition = value.indexOf("@");
                var dotposition = value.lastIndexOf(".");
                if (
                  atposition < 1 ||
                  dotposition < atposition + 2 ||
                  dotposition + 2 >= value.length
                ) {
                  //$.alert("نشانی پست الکترونیک  نامعتبر است");
                  iziToast.error({
                    title: "هشدار",
                    message: "نشانی پست الکترونیک  نامعتبر است",
                    iconText: "error_outline",
                  });
                  document
                    .getElementById("ctl00_PageContent_txtEmailAddress")
                    .focus();
                  return false;
                }
              } else {
                return true;
              }
              return true;
            }
            function nationalCode_Completed(result) {
              if (getXmlNodesByTagName(result, "valid_caption")[0] == "false") {
                // alert('کد ملی وارده معتبر نمی باشد.');
                iziToast.error({
                  title: "هشدار",
                  message: "کد ملی وارده معتبر نمی باشد.",
                  iconText: "error_outline",
                });
                document.getElementById(
                  "ctl00_PageContent_txtNationalCode"
                ).value = "";
                document
                  .getElementById("ctl00_PageContent_txtNationalCode")
                  .focus();
                return false;
              } else {
                return true;
              }
            }
          </script>
          <script type="text/javascript">
            var nCntSelectNotConfirm = 0;
            function OnTreeClick(evt) {
              var src =
                window.event != window.undefined
                  ? window.event.srcElement
                  : evt.target;
              var isChkBoxClick =
                src.tagName.toLowerCase() == "input" && src.type == "checkbox";
              if (isChkBoxClick) {
                if (src.checked == true) {
                  nCntSelectNotConfirm += 1;
                } else {
                  nCntSelectNotConfirm -= 1;
                }
                document.getElementById("lblCountSelectNotConfirm").innerHTML =
                  nCntSelectNotConfirm.toString() + " آیتم ";
              }
              if (src.tagName.toLowerCase() == "span") {
                var id = src.parentElement.id;
                // با کلیک روی کپشنی که با چک باکس شروع شده است، چک باکس هم انتخاب شود
                var cGetId = id.replace("TreeMenut", "TreeMenun") + "CheckBox";
                var ctrl = document.getElementById(cGetId);
                if (ctrl != null) {
                  if (ctrl.checked == true) {
                    ctrl.checked = false;
                    nCntSelectNotConfirm -= 1;
                  } else {
                    ctrl.checked = true;
                    nCntSelectNotConfirm += 1;
                  }
                  document.getElementById(
                    "lblCountSelectNotConfirm"
                  ).innerHTML = nCntSelectNotConfirm.toString() + " آیتم ";
                }
                return false;
              }
              if (src.tagName.toLowerCase() == "a") {
                return false;
              }
            }
          </script>
